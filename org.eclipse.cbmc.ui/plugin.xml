<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.eclipse.cbmc.ui.view.category"
            name="CBMC">
      </category>
      <view
            category="org.eclipse.cbmc.ui.view.category"
            class="org.eclipse.internal.cbmc.view.PropertiesView"
            icon="icons/sample.gif"
            id="org.eclipse.cbmc.ui.view.properties"
            name="CBMC">
      </view>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="org.eclipse.internal.cbmc.launcher.LaunchConfigurationDelegate"
            id="org.eclipse.cbmc.launchConfiguration"
            modes="run"
            name="CBMC"
            category="org.eclipse.ui.externaltools"
            public="true">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="org.eclipse.internal.cbmc.launcher.LaunchConfigurationTab"
            description="execute CBMC"
            id="org.eclipse.cbmc.launching.configurationTab"
            type="org.eclipse.cbmc.launchConfiguration">
            <launchMode
               mode="run">
         </launchMode>
      </launchConfigurationTabGroup>
   </extension>
   <extension
         id="org.eclipse.cbmc"
         point="org.eclipse.ui.menus">
   </extension>
</plugin>
